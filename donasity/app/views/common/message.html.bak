{$msgValues=NULL}
{function="getMessage($msgValues)"}
{if="$msgValues!=NULL"}
    <div id="msgSection" class="margin-top-10">
            {if condition="$msgValues!=null"}
                {if condition="$msgValues.msgType==1"}
                    <div class="alert alert-danger text-center">
                        <span>{$msgValues.ErrorMsg}</span>
                    </div>                            
                    {elseif condition="$msgValues.msgType==2"}
                    <div class="alert alert-success text-center">
                        <span>{$msgValues.ConfirmationMsg}</span>
                    </div>
                {/if}
            {/if}
    </div>
{/if}